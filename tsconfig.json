{
  "compilerOptions": {
    "lib": ["es7", "dom", "es2017.object"],
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "es6",
    "jsx": "preserve"
  },
  "compileOnSave": true,
  "include": ["src"],
  "exclude": [".vscode", "node_modules", "webapp"]
}
