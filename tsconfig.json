{
  "compilerOptions": {
    "lib": ["es6", "dom", "es2017.object"],
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "es6",
    "jsx": "preserve"
  },
  "compileOnSave": true,
  "include": ["."]
}
